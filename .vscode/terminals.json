{
  "autorun": false,
  "autokill": true,
  "terminals": [
    {
      "name": "Postgres",
      "commands": [
        "pkill -9 -f postgres",
        "~/.asdf/shims/postgres"
      ]
    },
    {
      "name": "Elasticsearch",
      "commands": [
        "pkill -9 -f elasticsearch",
        "~/.asdf/shims/elasticsearch"
      ]
    }
  ]
}